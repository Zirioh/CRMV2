{
  "name": "crm-v2",
  "description": "A CRM system built with React, Next.js, and Laravel.",
  "type": "project",
  "require": {
    "php": "^7.4 || ^8.0",
    "laravel/framework": "^8.0",
    "laravel/sanctum": "^2.0",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/tinker": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "php artisan vendor:publish --tag=laravel-assets --ansi"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}